<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>BidDetails</servlet-name>
  	<display-name>BidDetails</display-name>
  	<description></description>
  	<servlet-class>bidFeature.BidDetails</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>sellerItems</servlet-name>
  	<display-name>sellerItems</display-name>
  	<description></description>
  	<servlet-class>bidFeature.sellerItems</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>AddNewSellerItem</servlet-name>
  	<display-name>AddNewSellerItem</display-name>
  	<description></description>
  	<servlet-class>bidFeature.AddNewSellerItem</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>BidDetails</servlet-name>
  	<url-pattern>/BidDetails</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>sellerItems</servlet-name>
  	<url-pattern>/sellerItems</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>AddNewSellerItem</servlet-name>
  	<url-pattern>/AddNewSellerItem</url-pattern>
  </servlet-mapping>
</web-app>
